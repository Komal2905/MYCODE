#import <Foundation/Foundation.h>

int main(int argc, const char* argv[]){
    NSAutoreleasePool *pool = [[NSAutoreleasePool alloc] init];
    NSString *first = @"komal";
NSString *last = @"kamble";

NSString *fullName = [[first stringByAppendingString:@" "] stringByAppendingString:last];

NSLog(@"%@", fullName);

    [pool drain];
    return 0;
}
