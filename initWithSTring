#import <Foundation/Foundation.h>

int main(int argc, const char* argv[]){
    NSAutoreleasePool *pool = [[NSAutoreleasePool alloc] init];
    NSString *first = @"komal";

NSString *copy = [[NSString alloc] initWithString:first];

NSLog(@"%@ is a copy of %@", copy, first);

    [pool drain];
    return 0;
}
